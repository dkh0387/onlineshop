<?xml version="1.0" encoding="utf-8"?>
<t:layoutdefinition xmlns:t="http://www.CaptainCasa.com/controllibrary/t" >

<t:beanprocessing id="g_1" beanbinding="#{d.UsermanagementUI}" />
<t:rowtitlebar id="g_2" text="#{d.UsermanagementUI.pageTitle}" />
<t:rowheader id="g_3" />
<t:rowbodypane id="g_4" >
<t:row id="g_8" >
<t:foldablepane id="g_9" width="100%" >
<t:rowdistance id="g_10" height="5" />
<t:row id="g_11" >
<t:coldistance id="g_29" width="100%" />
<t:button id="g_12" actionListener="#{d.UsermanagementUI.onAddUser}" image="/de/dkh/onlineshop/images/add.gif" />
<t:coldistance id="g_14" width="10" />
<t:button id="g_13" actionListener="#{d.UsermanagementUI.onRemoveSelectedUsers}" image="/de/dkh/onlineshop/images/delete.gif" />
<t:coldistance id="g_15" width="10" />
<t:button id="g_31" actionListener="#{d.UsermanagementUI.onReload}" image="/de/dkh/onlineshop/images/refresh.gif" />
<t:coldistance id="g_6" width="10" />
<t:button id="g_7" actionListener="#{d.UsermanagementUI.onOpenProdCatalog}" image="/de/dkh/onlineshop/images/sortiment.png" />
<t:coldistance id="g_38" width="10" />
<t:button id="g_5" actionListener="#{d.UsermanagementUI.onOpenShoppingcart}" image="/de/dkh/onlineshop/images/einkaufswagen.png" />
</t:row>
<t:rowdistance id="g_41" height="10" />
<t:row id="g_33" >
<t:label id="g_34" font="posture:italic" text="Select one or more rows by pressing and holding the Strg-key" width="100%" />
</t:row>
</t:foldablepane>
</t:row>
<t:row id="g_39" >
<t:fixgrid id="g_16" height="100%" multiselect="true" objectbinding="#{d.UsermanagementUI.rows}" width="100%" >
<t:gridcol id="g_17" text="#{d.UsermanagementUI.colName}" width="100%" >
<t:label id="g_23" text=".{user.name}" />
</t:gridcol>
<t:gridcol id="g_20" text="#{d.UsermanagementUI.colAdress}" width="100%" >
<t:label id="g_25" text=".{user.adress}" />
</t:gridcol>
<t:gridcol id="g_21" text="#{d.UsermanagementUI.colMail}" width="100%" >
<t:label id="g_27" text=".{user.mail}" />
</t:gridcol>
<t:gridcol id="g_18" text="#{d.UsermanagementUI.colBday}" width="100%" >
<t:formattedfield id="g_36" format="date" value=".{user.bday}" />
</t:gridcol>
</t:fixgrid>
</t:row>
</t:rowbodypane>

</t:layoutdefinition>